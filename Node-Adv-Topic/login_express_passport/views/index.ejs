<% if (!user) { %>
	<h2>Welcome! Please log in.</h2>
	<h2><a href="/login">LogIn</a></h2>
<% } else { %>
	<h2>Hello, <%= user.username %>.</h2>
	<ul>
		<h2><li>Username: <%= user.username %></li></h2>
		<h2><li>Password: <%= user.password %></li></h2>
		<h2><li>Email: <%= user.email %></li></h2>
	</ul>
	<h2><a href="/logout">LogOut</a></h2>
<% } %>
